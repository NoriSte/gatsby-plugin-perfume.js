{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "2.9.11",
    "gatsby-image": "2.1.4",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-offline": "2.1.3",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.1.9",
    "gatsby-source-filesystem": "2.0.43",
    "gatsby-transformer-sharp": "2.1.21",
    "perfume.js": "2.1.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "cypress-wait-until": "1.2.2",
    "prettier": "1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve -p 8004",
    "plugin:link": "rm -f ./node_modules/gatsby-plugin-perfume.js && ln -s ../../../ ./node_modules/gatsby-plugin-perfume.js",
    "plugin:copy": "rm -rf ./node_modules/gatsby-plugin-perfume.js && mkdir ./node_modules/gatsby-plugin-perfume.js && \\cp ../../{gatsby-ssr.js,package.json,index.js,utils.js} ./node_modules/gatsby-plugin-perfume.js",
    "test:prepare": "yarn install && npm run plugin:link && npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
